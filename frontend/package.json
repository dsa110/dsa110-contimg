{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "Frontend application for managing provenance and error handling.",
  "main": "src/index.tsx",
  "scripts": {
    "ensure-port": "node scripts/ensure-port.cjs",
    "dev": "node scripts/ensure-port.cjs && vite",
    "dev:unsafe": "vite --host 0.0.0.0",
    "start": "node scripts/ensure-port.cjs && vite",
    "build": "vite build",
    "build:no-check": "vite build --emptyOutDir",
    "build:scratch": "TMPDIR=/scratch vite build --emptyOutDir && echo 'Build complete'",
    "build:analyze": "vite build --mode analyze 2>&1 | tee build-report.txt && echo 'Bundle report saved to build-report.txt'",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ci": "vitest run",
    "test:alignment": "vitest run src/api/alignment.test.ts",
    "test:alignment:live": "INTEGRATION_TEST=true vitest run src/api/alignment.test.ts",
    "test:alignment:check": "node scripts/check-api-alignment.cjs",
    "test:alignment:check:verbose": "node scripts/check-api-alignment.cjs --verbose",
    "test:alignment:check:all": "node scripts/check-api-alignment.cjs --all",
    "test:alignment:check:all:verbose": "node scripts/check-api-alignment.cjs --all --verbose",
    "type-check": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "storybook": "node scripts/ensure-storybook-port.cjs && storybook dev -p 6100 --no-open",
    "storybook:open": "node scripts/ensure-storybook-port.cjs && storybook dev -p 6100",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook --url http://localhost:6100",
    "test:storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook && npx http-server storybook-static --port 6100 --silent\" \"wait-on http://localhost:6100 && npm run test:storybook\"",
    "test:storybook:update": "test-storybook --url http://localhost:6100 --updateSnapshot",
    "openapi:fetch": "node scripts/fetch-openapi.cjs",
    "openapi:generate": "npx openapi-typescript http://localhost:8000/api/openapi.json -o src/api/generated/api.d.ts",
    "openapi:generate:file": "npx openapi-typescript src/api/openapi.json -o src/api/generated/api.d.ts",
    "openapi:sync": "npm run openapi:fetch && npm run openapi:generate:file"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.17.0",
    "@tanstack/react-query-devtools": "^5.91.1",
    "aladin-lite": "file:vendor/aladin-lite-3.7.3-beta.tgz",
    "axios": "^1.6.0",
    "d3": "^7.9.0",
    "d3-celestial": "^0.7.35",
    "d3-geo-projection": "^4.0.0",
    "date-fns": "^4.1.0",
    "echarts": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.6",
    "typescript": "^5.3.0",
    "zod": "^3.25.76",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.2",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-onboarding": "^10.1.2",
    "@storybook/react-vite": "^10.1.2",
    "@storybook/test-runner": "^0.24.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@types/d3": "^7.4.3",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-storybook": "^10.1.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jest-image-snapshot": "^6.5.1",
    "jsdom": "^22.1.0",
    "msw": "^2.12.3",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.5.6",
    "puppeteer": "^24.32.0",
    "storybook": "^10.1.2",
    "tailwindcss": "^3.4.18",
    "vite": "^7.2.4",
    "vitest": "^4.0.14",
    "wait-on": "^9.0.3"
  },
  "license": "MIT"
}
